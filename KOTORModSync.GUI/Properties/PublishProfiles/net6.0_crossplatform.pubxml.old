<Project>
    <PropertyGroup>
        <Configuration>Release</Configuration>
        <PublishDir>bin\publish\net6.0</PublishDir>
        <PublishProtocol>FileSystem</PublishProtocol>
        <_TargetId>Folder</_TargetId>
        <TargetFramework>net6.0</TargetFramework>
        <!-- net462 needs to explicitly be x64 and x86, other net versions are AnyCPU. See custom build action for Avalonia.Win32.dll in KOTORModSync.csproj-->
        <Platform>AnyCPU</Platform>
        <RuntimeIdentifiers>win-x86;win-x64;win7-x64;osx-x64;linux-x64</RuntimeIdentifiers>
    </PropertyGroup>
</Project>
